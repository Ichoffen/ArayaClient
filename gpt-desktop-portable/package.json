{
  "name": "gpt-desktop-portable",
  "version": "0.2.3",
  "private": true,
  "type": "module",
  "main": "main.js",
  "build": {
    "appId": "com.motorbuild.gptdesktop",
    "productName": "GPT Desktop Portable",
    "win": {
      "target": [
        "portable"
      ]
    },
    "files": [
      "main.js",
      "preload.js",
      "renderer/**/*",
      "api/**/*",
      "attachments/**/*"
    ]
  },
  "scripts": {
    "dev": "electron .",
    "dist": "electron-builder -w portable",
    "pack": "electron-builder --dir"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mime-types": "^2.1.35",
    "node-fetch": "^3.3.2",
    "lowdb": "^7.0.1"
  },
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-builder": "^24.13.3"
  }
}